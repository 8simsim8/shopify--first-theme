{%- for block in section.blocks -%}
  {%- case block.type -%}
    {%- when '404-main' -%}
    <section>
      <div>{{block.settings.text | escape}}</div>
    </section>
  {%- endcase -%}
{%- endfor -%}

{% schema %}
  {
    "name": "404 Main section",
    "settings": [{
		"type": "text",
        "id": "text",
		"default": "404 page section",
        "label": "Change text 404 page section"
	}]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
{% endjavascript %}
